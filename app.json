{
  "expo": {
    "name": "MyCV-Application",
    "slug": "MyCV-Application",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/images/icon_logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/images/logo_for_app_loading.png",
      "resizeMode": "contain",
      "backgroundColor": "#F5F5F5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
